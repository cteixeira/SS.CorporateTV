@model IEnumerable<SS.CorporateTV.BO.Models.ProgramacaoModel>

@if (Model != null && Model.Any())
{
    foreach (var item in Model)
    {
        <tr id="@item.ProgramacaoID" class="linha">
            <td class="col-md-2">
                @Html.DisplayFor(m => item.Designacao)
            </td>
        </tr>
    }
}
else
{
    <tr>
        <td colspan="5" class="trNoResults">
            @SS.CorporateTV.BO.Resources.Geral.SemResultados
        </td>
    </tr>
}
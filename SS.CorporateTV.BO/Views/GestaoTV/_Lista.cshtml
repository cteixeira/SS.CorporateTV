@model IEnumerable<SS.CorporateTV.BO.Models.TvModel>

@if (Model != null && Model.Any())
{
    foreach (var item in Model)
    {
        <tr id="@item.ID" class="linha">
            <td class="col-md-6">
                @Html.DisplayFor(m => item.Designacao)
            </td>

            <td class="col-md-6">
                @Html.DisplayFor(m => item.ProgramacaoDesignacao)
            </td>
        </tr>
    }
}
else
{
    <tr>
        <td colspan="5" class="trNoResults">
            @SS.CorporateTV.BO.Resources.Geral.SemResultados
        </td>
    </tr>
}